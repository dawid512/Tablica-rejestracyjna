@model IEnumerable<tablica_rejestracyjna.Data.wojewodztwa>


<li class="dropdown">
<a asp-controller="tablice"
   asp-action="Index"
   class="dropdown-toggle" data-toggle="dropdown">Województwa<b class="caret"></b></a>
<ul class="dropdown-menu">
    @foreach (var category in Model)
    {
        <li>
            <a asp-controller="Product" asp-action="List"
               asp-route-category="@category.Name">@category.Name</a>
        </li>
    }
    <li class="divider"></li>
   
</ul>
</li>